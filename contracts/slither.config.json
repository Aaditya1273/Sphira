{
  "filter_paths": ["node_modules", "test", "scripts"],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_dependencies": true,
  "show_ignored_findings": false,
  "detectors_to_exclude": ["pragma", "solc-version", "naming-convention"],
  "detectors_to_run": [
    "reentrancy-eth",
    "reentrancy-no-eth",
    "uninitialized-state",
    "uninitialized-storage",
    "arbitrary-send",
    "controlled-delegatecall",
    "weak-prng",
    "suicidal",
    "assembly",
    "assert-state-change",
    "boolean-equal",
    "dangerous-unary",
    "deprecated-standards",
    "divide-before-multiply",
    "enum-conversion",
    "erc20-interface",
    "erc721-interface",
    "incorrect-equality",
    "locked-ether",
    "mapping-deletion",
    "shadowing-abstract",
    "shadowing-builtin",
    "shadowing-local",
    "shadowing-state",
    "timestamp",
    "too-many-digits",
    "tx-origin",
    "unchecked-lowlevel",
    "unchecked-send",
    "uninitialized-local",
    "unused-return",
    "unused-state",
    "costly-loop",
    "dead-code",
    "reentrancy-benign",
    "reentrancy-events",
    "similar-names",
    "too-many-digits",
    "constable-states",
    "external-function",
    "immutable-states",
    "var-read-using-this"
  ]
}
